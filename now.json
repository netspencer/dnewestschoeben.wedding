{
  "version": 2,
  "name": "wedding",
  "alias": "dylanandspencer.wedding",
  "builds": [
    {
      "src": "./www/package.json",
      "use": "@now/static-build@canary"
    },
    {
      "src": "./www/static/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/www/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ],
  "env": {
    "IS_NOW": "true"
  }
}
